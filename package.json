    "bcryptjs": "^3.0.3",
    "bullmq": "^5.66.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron-parser": "^5.5.0",
    "croner": "^8.0.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.26",
    "ioredis": "^5.9.2",
    "lucide-react": "^0.562.0",
    "mathjs": "^15.1.0",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.30",
