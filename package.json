{
  "name": "orderhubnow",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:reset": "bash scripts/dev-reset.sh",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "verify": "npm run type-check && npm run lint && npm run check:hydration && npm run build",
    "check:hydration": "npx tsx scripts/check-hydration.ts",
    "check:hydration:fix": "npx tsx scripts/check-hydration.ts --fix",
    "check:hydration:strict": "npx tsx scripts/check-hydration.ts --strict",
    "check-migrations": "tsx scripts/check-migrations.ts",
    "check-schema-drift": "bash scripts/check-schema-drift.sh",
    "apply-migration": "bash scripts/apply-migration.sh",
    "deploy": "bash scripts/deploy.sh",
    "db:tunnel": "bash scripts/db-tunnel.sh",
    "prepare": "husky"
  },
  "dependencies": {
